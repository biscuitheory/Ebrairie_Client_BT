import { handleChange } from '../login';

describe('Ma fonction handleChange', () => {
  test('setEmail si email saisi dans input', () => {
    const expectedData = 'jacques@email.com';
    expect(handleChange('email', 'jacques@email.com')).toEqual(expectedData);
  });

  // test('Do not setEmail si pas email saisi dans input', () => {});
  // const expectedState = 'jacques@email.com';
  // expect(handleChange('email', 'jacquesemail.com')).toEqual(expectedState);
});
